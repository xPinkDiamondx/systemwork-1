        <h3>練習２</h3>
        <h4>(1)1行3列の表</h4>

            <table border = "1">
        <%  for(int x = 0 ; x <= 2 ; x ++){%>
        <th>test</th>
        <%  }%>


		<h4>(2)縦に20行</h4>

            <table border = "1">
        <%  for(int y = 0 ; y >= 19 ; y ++){%>
        	<tr>test</tr>
        <%  }%>


        <h4>(3)20行3行の表</h4>


            <table border = "1">
        <%  for(int x = 0 ; x <= 19 ; x ++){%>
        	<th>test</th>
        	<%  for(int y = 0 ; y <= 2 ; y ++){%>
				<tr>test</tr>
    		<%  }%>
        <%  }%>


